# Python program to split a string and join 
def split_string(str):
	list_str=str.split(' ')
	return list_str
def join_string(list_str):
	string='-'.join(list_str)
	return str
if __name__=='__main__':
	str='GitHub'
	#Splitting a string
	list_str=split_string(str)
	print(list_string)
	#Join list of strings 
	new_str=join_string(list_str)
	print(new_str)
